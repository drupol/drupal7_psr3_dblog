{
    "name": "drupol/drupal7_psr3_watchdog",
    "type": "library",
    "description": "A PSR3 logger using Drupal 7 watchdog.",
    "keywords": [
        "log",
        "logging",
        "psr-3",
        "drupal",
        "watchdog"
    ],
    "homepage": "http://github.com/drupol/drupal7_psr3_watchdog",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "Pol Dellaiera",
            "email": "pol.dellaiera@protonmail.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "psr/log": "^1.0"
    },
    "provide": {
        "psr/log-implementation": "1.0.0"
    },
    "require-dev": {
        "drupal/drupal": "^7",
        "drupol/php-conventions": "^1.5",
        "drupol/phpspec-code-coverage": "^4",
        "phpspec/phpspec": "^4",
        "phptaskman/changelog": "^0.1",
        "scrutinizer/ocular": "^1.6"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "drupol\\drupal7_psr3_watchdog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "drupol\\drupal7_psr3_watchdog\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
    ],
    "scripts": {
        "grumphp": "./vendor/bin/grumphp run",
        "phpspec": "./vendor/bin/phpspec run"
    }
}
